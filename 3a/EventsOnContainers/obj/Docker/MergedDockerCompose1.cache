networks:
  backend: {}
  frontend: {}
services:
  eventcatalog:
    build:
      context: C:\Users\tisha\Desktop\Bootcamp\Assignment 3\3a\EventsOnContainers\EventCatalogAPI
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: EventCatalogAPI
      target: base
    container_name: eventscatalogapi
    depends_on:
      mssqlserver:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseName: EventsDb
      DatabasePassword: EventApi(!)
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: events/eventcatalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/netcoreapp3.1/EventCatalogAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 7005
      target: 80
    tty: true
    volumes:
    - C:\Users\tisha\Desktop\Bootcamp\Assignment 3\3a\EventsOnContainers\EventCatalogAPI:/app:rw
    - C:\Users\tisha\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\tisha\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\tisha\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages2:ro
    - C:\Users\tisha\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\tisha\Desktop\Bootcamp\Assignment 3\3a\EventsOnContainers\EventCatalogAPI:/src:rw
  mssqlserver:
    container_name: eventbritesqlserver
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: EventApi(!)
    image: mcr.microsoft.com/mssql/server:latest
    networks:
      backend: {}
    ports:
    - published: 1450
      target: 1433
version: '3.4'